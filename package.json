{
  "name": "abc-stock-handler",
  "version": "0.0.0",
  "description": "a stock handler for products a,b and c",
  "main": "app.js",
  "license": "Unlicense",
  "scripts": {
    "lint": "eslint '*/**/*.js' --quiet",
    "lint-fix": "eslint '*/**/*.js' --quiet --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*js' --write",
    "start": "NODE_ENV=production node build/src/app.js",
    "dev": "NODE_ENV=development nodemon build/src/app.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --quiet"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "express-prom-bundle": "^6.1.0",
    "express-rate-limit": "^5.1.3",
    "graphql": "^15.3.0",
    "knex": "^0.21.2",
    "pg": "^8.3.0",
    "prom-client": "^12.0.0",
    "rate-limiter-flexible": "^2.1.10",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
