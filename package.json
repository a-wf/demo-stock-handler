{
  "name": "abc-stock-handler",
  "version": "0.0.0",
  "description": "a stock handler for products a,b and c",
  "main": "app.js",
  "license": "Unlicense",
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts}' --quiet",
    "lint-fix": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{js,ts}' --write",
    "start": "NODE_ENV=production node build/src/app.js",
    "dev": "NODE_ENV=development nodemon build/src/app.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "express-prom-bundle": "^6.1.0",
    "express-rate-limit": "^5.1.3",
    "graphql": "^15.3.0",
    "prom-client": "^12.0.0",
    "rate-limiter-flexible": "^2.1.10",
    "winston": "^3.3.3"
  }
}
