{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 1,
    "strict": ["error", "global"],
    "arrow-parens": ["error", "as-needed"]
  }
}
